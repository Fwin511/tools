{
    "name": "feiyun/auto-filter",
    "description": "Laravel/Hyperf 模型自动筛选扩展包，根据请求参数和字段类型自动构建查询条件",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "hyperf",
        "filter",
        "query",
        "database",
        "eloquent",
        "auto-filter"
    ],
    "authors": [
        {
            "name": "Sally",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Feiyun\\AutoFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Feiyun\\AutoFilter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Feiyun\\AutoFilter\\Providers\\LaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
